execute as @e[type=minecraft:snowball] at @s if entity @e[type=ghast,distance=..6] run summon creeper ~ ~ ~ {Fuse:0,ExplosionRadius:4b,CustomName:"{\"text\":\"Aqua Grenade\",\"color\":\"aqua\"}"}
execute as @e[type=minecraft:snowball] at @s if entity @e[type=ghast,distance=..6] run kill @s

# Detonate the egg if it comes within 3 blocks of an entity (6 blocks for ghasts)
execute as @e[type=minecraft:egg] at @s if entity @e[tag=grenade,distance=..3] run summon creeper ~ ~ ~ {Fuse:0,ExplosionRadius:3b,CustomName:"{\"text\":\"Eggsplosion\",\"color\":\"aqua\"}"}
execute as @e[type=minecraft:egg] at @s if entity @e[tag=grenade,distance=..3] run kill @s
execute as @e[type=minecraft:egg] at @s if entity @e[type=ghast,distance=..6] run summon creeper ~ ~ ~ {Fuse:0,ExplosionRadius:4b,CustomName:"{\"text\":\"Eggsplosion\",\"color\":\"aqua\"}"}
execute as @e[type=minecraft:egg] at @s if entity @e[type=ghast,distance=..6] run kill @s

